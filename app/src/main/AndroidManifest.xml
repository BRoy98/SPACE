<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hometsolutions.space">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <!-- android:icon="@mipmap/ic_launcher" -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_beta"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activitys.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activitys.SplashActivityFirstLaunch"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activitys.SplashActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.EditComponentsActivity"></activity>
        <activity android:name=".Activitys.NewConnectionActivity"></activity>
    </application>

</manifest>